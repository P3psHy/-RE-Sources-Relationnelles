{% extends 'base.html.twig' %}

{% block title %}EtatRessource index{% endblock %}

{% block body %}
    <h1>EtatRessource index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for etat_ressource in etat_ressources %}
            <tr>
                <td>{{ etat_ressource.id }}</td>
                <td>{{ etat_ressource.nom }}</td>
                <td>
                    <a href="{{ path('app_etat_ressource_show', {'id': etat_ressource.id}) }}">show</a>
                    <a href="{{ path('app_etat_ressource_edit', {'id': etat_ressource.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_etat_ressource_new') }}">Create new</a>
{% endblock %}
